@article{blotnerTraitDeceptionDetection2023,
  title = {It Is Double Pleasure to Deceive the Deceiver: {{Machiavellianism}} Is Associated with Producing but Not Necessarily with Falling for Bullshit},
  shorttitle = {It Is Double Pleasure to Deceive the Deceiver},
  author = {Bl{\"o}tner, Christian and Bergold, Sebastian},
