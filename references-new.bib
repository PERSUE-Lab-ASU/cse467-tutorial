@article{lwinPrivacyDyadAntecedents,
  title = {The {{Privacy Dyad}}: {{Antecedents}} of {{Promotion-}} and {{Prevention-focused Online Privacy Behaviors}} and the {{Mediating Role}} of {{Trust}} and {{Privacy Concern}}},
  shorttitle = {The Privacy Dyad},
  author = {Lwin, May O. and Wirtz, Jochen and Stanaland, Andrea J. S.},
  year = {2016},
